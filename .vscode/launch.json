  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/root/main.go",
      "env": {
        "CUMULUS_AUTH_DISABLED": "FALSE",
        "CUMULUS_DBUSER": "postgres",
        "CUMULUS_DBPASS": "postgres",
        "CUMULUS_DBNAME": "postgres",
        "CUMULUS_DBHOST": "localhost",
        "CUMULUS_DBSSLMODE": "disable",
        "CUMULUS_LAMBDACONTEXT": "FALSE",
        "CUMULUS_ASYNC_ENGINE_ACQUISITION": "MOCK",
        "CUMULUS_ASYNC_ENGINE_ACQUISITION_SNS_TOPIC": "",
        "CUMULUS_ASYNC_ENGINE_PACKAGER": "MOCK",
        "CUMULUS_ASYNC_ENGINE_PACKAGER_SNS_TOPIC": ""
      },
      "args": []
    }
  ]
}
